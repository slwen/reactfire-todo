{
  "name": "reactfire-todo",
  "version": "0.0.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/slwen/reactfire-todo.git"
  },
  "description": "Todo application built with React and Firebase, written in ES6.",
  "main": "app.js",
  "engines": {
    "node": "~7.0.0"
  },
  "scripts": {
    "setup": "npm install && gulp serve",
    "start": "gulp clean && gulp serve",
    "build": "gulp clean && gulp --production",
    "test": "mocha `find . -name '*-test.js' -not -path '*node_modules*'`",
    "test-watch": "npm test -- --watch",
    "lint": "eslint src",
    "deploy": "gulp --production && gulp deploy"
  },
  "author": "Sam Enoka",
  "license": "ISC",
  "dependencies": {
    "firebase": "^3.7.3",
    "local-storage": "^1.4.2",
    "lodash": "^4.6.1",
    "react": "^15.0.1",
    "react-addons-css-transition-group": "^15.0.1",
    "react-dom": "^15.0.0",
    "reactfire": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-rewire": "^1.0.0-rc-1",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.2.0",
    "browser-sync": "^2.8.2",
    "browserify": "^14.1.0",
    "chai": "^3.5.0",
    "enzyme": "^2.1.0",
    "eslint": "~3.19.0",
    "eslint-plugin-react": "^6.0.0",
    "gh-pages-deploy": "^0.4.1",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-eslint": "^3.0.0",
    "gulp-gh-pages": "^0.5.2",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^3.0.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-sass": "^3.0.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.6",
    "jsdom": "^9.8.2",
    "mocha": "^3.0.0",
    "react-addons-test-utils": "^15.0.1",
    "rimraf": "^2.4.2",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.8.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.3.1"
  }
}
